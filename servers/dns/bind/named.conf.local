zone "example.com" IN {
type master;
file "/etc/bind/forward.example.com";
};

zone "esoftthings.com" IN {
type master;
file "/etc/bind/esoftthings.com";
};

zone "1.10.168.in-addr.arpa" IN {
type master;
file "/etc/bind/reverse.example.com";
};

